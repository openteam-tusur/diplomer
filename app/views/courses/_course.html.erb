<tr>
  <td>
    <span></span>
    <%= course.title %>
    <%= link_to t(:delete), diploma_course_path(course.diploma, course), :confirm => t('Are you sure?'), :method => :delete, :remote => true, :class=>'delete_link' %>
    <%= link_to t(:edit), edit_diploma_course_path(course.diploma, course), :class => 'edit_link' %>
  </td>
  <td><%= course.eng_title %></td>
  <td><%= course.credits %></td>
  <td><%= course.hours %></td>
  <td><%= course.human_grade %></td>
</tr>

